@{
    ViewData["Title"] = "Delete";
}
<div class="text-center">
    <h2 class="display-4">Удаление склада</h2>
</div>
<form method="post">
    <div class="row">
        <div class="row">
            <div class="col-4">Склад:</div>
            <div class="col-8">
                <select id="storehouseId" name="storehouseId" class="form-control" asp-items="@(new SelectList(@ViewBag.Storehouse,"Id", "StorehouseName"))">
                </select>
            </div>
        </div>
        <div class="row">
            <div class="col-4"></div>
            <div class="col-8">
                <input type="submit" value="Удалить" class="btn btn-primary" />
            </div>
        </div>
    </div>
</form>